{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
      "strict": "off",
      "object-curly-newline": "off",
      "class-methods-use-this": "warn",
      "no-unused-vars": "off",
      "max-len": "off"
    },
  "ignorePatterns": [
    "node_modules/"
  ]
}
